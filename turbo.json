{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalDependencies": [".env"],
  "tasks": {
    "generate": {
      "dependsOn": [],
      "cache": false
    },
    "migrate": {
      "dependsOn": [],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "generated/**"],
      "env": ["PRISMA_BINARY"],
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
